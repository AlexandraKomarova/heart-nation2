  "bugs": {
    "url": "https://github.com/AlexandraKomarova/dev-connect/issues"
  },
  "homepage": "https://github.com/AlexandraKomarova/dev-connect#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "chalk": "^3.0.0",
    "config": "^3.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.4.0",
    "gravatar": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.7",
    "request": "^2.88.2",
    "if-env": "^1.0.4"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "env-cmd": "^10.1.0"
  }
